###############################################################################
#                               NetCOBOL
###############################################################################

COBFLAGS=
LINKOPTION =

ALL : ..\SAMPLE29_EXE\SAMPLE29.EXE ..\SAMPLE29_EXE\TSUUSHIN.EXE

REBUILD : CLEAN ALL
CLEAN :
	DEL "..\SAMPLE29_EXE\SAMPLE29.EXE"
	DEL "SAMPLE29.OBJ"
	DEL "..\SAMPLE29_EXE\TSUUSHIN.EXE"
	DEL "TSUUSHIN.OBJ"

"SAMPLE29.OBJ" : "..\SAMPLE29_EXE\SAMPLE29.COB" "..\SAMPLE29_EXE\Prm_rec.cbl" "MAKEFILE"
	COBOL32.EXE -do. -I..\SAMPLE29_EXE $(COBFLAGS) -M "..\SAMPLE29_EXE\SAMPLE29.COB"

"..\SAMPLE29_EXE\SAMPLE29.EXE" : "SAMPLE29.OBJ"
	LINK.EXE $(LINKOPTION) \
	/DYNAMICBASE:NO \
	/OUT:"..\SAMPLE29_EXE\SAMPLE29.EXE" \
	"SAMPLE29.OBJ" \
	"F3BICIMP.LIB" \
	"MSVCRT.LIB" 


"TSUUSHIN.OBJ" : "..\SAMPLE29_TSUUSHIN\TSUUSHIN.COB" "..\SAMPLE29_EXE\Prm_rec.cbl" "MAKEFILE"
	COBOL32.EXE -do. -I..\SAMPLE29_EXE $(COBFLAGS) -M "..\SAMPLE29_TSUUSHIN\TSUUSHIN.COB"

"..\SAMPLE29_EXE\TSUUSHIN.EXE" : "TSUUSHIN.OBJ"
	LINK.EXE $(LINKOPTION) \
	/DYNAMICBASE:NO \
	/OUT:"..\SAMPLE29_EXE\TSUUSHIN.EXE" \
	"TSUUSHIN.OBJ" \
	"F3BICIMP.LIB" \
	"MSVCRT.LIB" 

