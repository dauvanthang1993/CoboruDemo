	COBOL32.EXE -i"SAMPLE19.CBI" -NM "MEMBER.COB"

"SALA_MEM.OBJ" : "SALA_MEM.COB" "MEMBER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "SALA_MEM.COB"

"BONU_MEM.OBJ" : "BONU_MEM.COB" "MEMBER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "BONU_MEM.COB"

"MEM_SET.OBJ" : "MEM_SET.COB" "MEMBERMASTER-CLASS.REP" "MEMBER-CLASS.REP" "SAMPLE19.CBI"
