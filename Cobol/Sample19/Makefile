COBFLAGS=
LINKOPTION =

ALL : MAIN.EXE

REBUILD : CLEAN ALL
CLEAN :
	DEL "MAIN.EXE" 
	DEL "MAIN.OBJ" 
	DEL "MAIN.LIB" 
	DEL "MAIN.EXP" 
	DEL "MANAGER.OBJ" 
	DEL "MANAGER-CLASS.REP" 
	DEL "SALA_MAN.OBJ" 
	DEL "BONU_MAN.OBJ" 
	DEL "MAN_SET.OBJ" 
	DEL "MAN_STOR.OBJ" 
	DEL "MEMBER.OBJ" 
	DEL "MEMBER-CLASS.REP" 
	DEL "SALA_MEM.OBJ" 
	DEL "BONU_MEM.OBJ" 
	DEL "MEM_SET.OBJ" 
	DEL "MEM_STOR.OBJ" 
	DEL "ALLMEM.OBJ" 
	DEL "ALLMEMBER-CLASS.REP" 
	DEL "SET.OBJ" 
	DEL "STORE.OBJ" 
	DEL "ALLMEM_M.OBJ" 
	DEL "MEMBERMASTER-CLASS.REP" 
	DEL "ADDRESS.OBJ" 
	DEL "ADDRESS-CLASS.REP" 
	DEL "ADDR_M.OBJ" 
	DEL "ADDRESSMASTER-CLASS.REP" 

"MAIN.OBJ" : "MAIN.COB" "ADDRESS-CLASS.REP" "MEMBER-CLASS.REP" "ALLMEMBER-CLASS.REP" "MANAGER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -M "MAIN.COB"

"MANAGER.OBJ" : "MANAGER.COB" "ALLMEMBER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "MANAGER.COB"

"MANAGER-CLASS.REP" : "MANAGER.COB" "ALLMEMBER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "MANAGER.COB"

"SALA_MAN.OBJ" : "SALA_MAN.COB" "MANAGER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "SALA_MAN.COB"

"BONU_MAN.OBJ" : "BONU_MAN.COB" "MANAGER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "BONU_MAN.COB"

"MAN_SET.OBJ" : "MAN_SET.COB" "MEMBERMASTER-CLASS.REP" "MANAGER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "MAN_SET.COB"

"MAN_STOR.OBJ" : "MAN_STOR.COB" "MEMBERMASTER-CLASS.REP" "MANAGER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "MAN_STOR.COB"

"MEMBER.OBJ" : "MEMBER.COB" "ALLMEMBER-CLASS.REP" "SAMPLE19.CBI"
