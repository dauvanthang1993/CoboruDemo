	COBOL32.EXE -i"SAMPLE19.CBI" -NM "ADDRESS.COB"

"ADDRESS-CLASS.REP" : "ADDRESS.COB" "ADDRESSMASTER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "ADDRESS.COB"

