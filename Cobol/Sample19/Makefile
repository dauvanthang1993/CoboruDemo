"MAIN.EXE" : "MAIN.OBJ" "MANAGER.OBJ" "SALA_MAN.OBJ" "BONU_MAN.OBJ" "MAN_SET.OBJ" "MAN_STOR.OBJ" "MEMBER.OBJ" "SALA_MEM.OBJ" "BONU_MEM.OBJ" "MEM_SET.OBJ" "MEM_STOR.OBJ" "ALLMEM.OBJ" "SET.OBJ" "STORE.OBJ" "ALLMEM_M.OBJ" "ADDRESS.OBJ" "ADDR_M.OBJ" 
	"LINK.EXE" $(LINKOPTION) \
	/DYNAMICBASE:NO \
	/OUT:"MAIN.EXE" \
	"MAIN.OBJ" "MANAGER.OBJ" "SALA_MAN.OBJ" \
        "BONU_MAN.OBJ" "MAN_SET.OBJ" "MAN_STOR.OBJ" \
        "MEMBER.OBJ" "SALA_MEM.OBJ" "BONU_MEM.OBJ" \
