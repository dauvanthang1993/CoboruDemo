	COBOL32.EXE -i"SAMPLE19.CBI" -NM "ALLMEM.COB"

"ALLMEMBER-CLASS.REP" : "ALLMEM.COB" "MEMBERMASTER-CLASS.REP" "ADDRESS-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "ALLMEM.COB"

"SET.OBJ" : "SET.COB" "MEMBERMASTER-CLASS.REP" "ADDRESS-CLASS.REP" "ALLMEMBER-CLASS.REP" "SAMPLE19.CBI"
	COBOL32.EXE -i"SAMPLE19.CBI" -NM "SET.COB"

