###############################################################################
#                               NetCOBOL
###############################################################################

COBFLAGS=
LINKOPTION =

ALL : SPECCTRL.DLL SAMPLE17.EXE

REBUILD : CLEAN ALL
CLEAN :
	DEL "SAMPLE17.EXE"
	DEL "SAMPLE17.LIB"
	DEL "SAMPLE17.EXP"
	DEL "SPECCTRL.DLL"
	DEL "SPECCTRL.LIB"
	DEL "SPECCTRL.EXP"
	DEL "SPEC.OBJ"
	DEL "仕様クラス.REP"
	DEL "SINGLETN.OBJ"
	DEL "シングルトン.REP"
	DEL "ERRORPUT.OBJ"
	DEL "エラークラス.REP"
	DEL "LISTITER.OBJ"
	DEL "リストイテレータ.REP"
	DEL "SPECCTRL.OBJ"
	DEL "仕様管理クラス.REP"
	DEL "ROOMSPEC.OBJ"
	DEL "会議室情報クラス.REP"
	DEL "ROOMCTRL.OBJ"
	DEL "会議室情報管理クラス.REP"
	DEL "RESERVE.OBJ"
	DEL "予約情報クラス.REP"
	DEL "TIMESTA.OBJ"
	DEL "予約状態−時間枠クラス.REP"
	DEL "ROOMSTA.OBJ"
	DEL "予約状態−会議室クラス.REP"
	DEL "DATESTA.OBJ"
	DEL "予約状態−日付クラス.REP"
	DEL "RSVCTRL.OBJ"
	DEL "予約管理クラス.REP"
	DEL "EXCELEDT.OBJ"
	DEL "MAIN.OBJ"

"SPEC.OBJ" : "..\SAMPLE17_DLL\SPEC.COB" "..\SAMPLE17_LIB\SPECINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_DLL\SPEC.COB"

"SINGLETN.OBJ" : "..\SAMPLE17_DLL\SINGLETN.COB" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_DLL\SINGLETN.COB"

"ERRORPUT.OBJ" : "..\SAMPLE17_DLL\ERRORPUT.COB" "シングルトン.rep" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_DLL\ERRORPUT.COB"

"LISTITER.OBJ" : "..\SAMPLE17_DLL\LISTITER.COB" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_DLL\LISTITER.COB"

"SPECCTRL.OBJ" : "..\SAMPLE17_DLL\SPECCTRL.COB" "シングルトン.rep" "仕様クラス.rep" "リストイテレータ.rep" "エラークラス.rep" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_DLL\SPECCTRL.COB"

"SPECCTRL.DLL" : "SPEC.OBJ" "SINGLETN.OBJ" "ERRORPUT.OBJ" "LISTITER.OBJ" "SPECCTRL.OBJ"
	"LINK.EXE" $(LINKOPTION) \
	/DYNAMICBASE:NO \
	/DLL /NOENTRY \
	/OUT:"SPECCTRL.DLL" \
	"SPEC.OBJ" \
	"SINGLETN.OBJ" \
	"ERRORPUT.OBJ" \
	"LISTITER.OBJ" \
	"SPECCTRL.OBJ" \
	"..\SAMPLE16\COLLECT.LIB" \
	"F3BICIMP.LIB"

"ROOMSPEC.OBJ" : "..\SAMPLE17_EXE\ROOMSPEC.COB" "仕様クラス.rep" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\ROOMSPEC.COB"

"ROOMCTRL.OBJ" : "..\SAMPLE17_EXE\ROOMCTRL.COB" "シングルトン.rep" "仕様クラス.rep" "リストイテレータ.rep" "エラークラス.rep" "仕様管理クラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\ROOMINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\ROOMCTRL.COB"

"RESERVE.OBJ" : "..\SAMPLE17_EXE\RESERVE.COB" "リストイテレータ.rep" "エラークラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\RESERVE.COB"

"TIMESTA.OBJ" : "..\SAMPLE17_EXE\TIMESTA.COB" "予約情報クラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\TIMESTA.COB"

"ROOMSTA.OBJ" : "..\SAMPLE17_EXE\ROOMSTA.COB" "予約状態−時間枠クラス.rep" "会議室情報クラス.rep" "リストイテレータ.rep" "エラークラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\ROOMSTA.COB"

"DATESTA.OBJ" : "..\SAMPLE17_EXE\DATESTA.COB" "リストイテレータ.rep" "エラークラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "..\SAMPLE17_LIB\ROOMINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\DATESTA.COB"

"RSVCTRL.OBJ" : "..\SAMPLE17_EXE\RSVCTRL.COB" "予約情報クラス.rep" "リストイテレータ.rep" "エラークラス.rep" "予約状態−日付クラス.REP" "予約状態−会議室クラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "..\SAMPLE17_LIB\ROOMINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\RSVCTRL.COB"

"EXCELEDT.OBJ" : "..\SAMPLE17_EXE\EXCELEDT.COB" "予約情報クラス.REP" "予約管理クラス.REP" "会議室情報管理クラス.REP" "リストイテレータ.rep" "エラークラス.rep" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "..\SAMPLE17_LIB\ROOMINFO.CBL" "MAKEFILE"
	COBOL32.EXE -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\EXCELEDT.COB"

"MAIN.OBJ" : "..\SAMPLE17_EXE\MAIN.COB" "予約管理クラス.REP" "会議室情報管理クラス.REP" "..\SAMPLE17_LIB\R_CONST.CBL" "..\SAMPLE17_LIB\RSVINFO.CBL" "..\SAMPLE17_LIB\ROOMINFO.CBL" "MAKEFILE"
	COBOL32.EXE -M -do. -dr. -I"..\SAMPLE17_LIB" -R"..\SAMPLE16" "..\SAMPLE17_EXE\MAIN.COB"

"SAMPLE17.EXE" : "ROOMSPEC.OBJ" "ROOMCTRL.OBJ" "RESERVE.OBJ" "TIMESTA.OBJ" "ROOMSTA.OBJ" "DATESTA.OBJ" "RSVCTRL.OBJ" "EXCELEDT.OBJ" "MAIN.OBJ" "SPECCTRL.DLL"
	LINK.EXE $(LINKOPTION) \
	/DYNAMICBASE:NO \
	/SUBSYSTEM:WINDOWS,5.01 \
	/OUT:"SAMPLE17.EXE" \
	"ROOMSPEC.OBJ" \
	"ROOMCTRL.OBJ" \
	"RESERVE.OBJ" \
	"TIMESTA.OBJ" \
	"ROOMSTA.OBJ" \
	"DATESTA.OBJ" \
	"RSVCTRL.OBJ" \
	"EXCELEDT.OBJ" \
	"MAIN.OBJ" \
	"..\SAMPLE16\COLLECT.LIB" \
	"SPECCTRL.LIB" \
	"MSVCRT.LIB" \
	"F3BICIMP.LIB" 


